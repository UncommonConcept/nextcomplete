{
  "plugins": [
    [
      "module-resolver", {
        "root": ["."],
        "alias": {
          "assets": "./assets",
          "components": "./components",
          "server": "./server"
        },
        "cwd": "babelrc"
    }],
    [
      "wrap-in-js",
      {
        "extensions": ["css$", "scss$"]
      }
    ]
  ],
  "presets": [
    "next/babel",
    "stage-0"
  ],
  "ignore": []
}
